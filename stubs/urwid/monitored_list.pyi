    def set_validate_contents_modified(self, callback: Any) -> None: ...
    def __delitem__(self, y: Any): ...
    def __setitem__(self, i: Any, y: Any): ...
    def __delslice__(self, i: Any, j: Any): ...
    def __setslice__(self, i: Any, j: Any, y: Any): ...
    def __imul__(self, n: Any): ...
    def append(self, item: Any): ...
    def extend(self, items: Any): ...
    def insert(self, index: Any, item: Any): ...
    def pop(self, index: int = ...): ...
    def remove(self, value: Any): ...
    def reverse(self): ...
    def sort(self, **kwargs: Any): ...
    def clear(self): ...
